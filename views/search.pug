extends layouts/main
include mixin/videoBlock

block content
    .search__header
        p Searching for: #{searchingBy}
    .home-videos
        if videos.length === 0 
            h4 No Videos!
        each item in videos
            +videoBlock({
                id: item.id,
                file: item.fileUrl,
                title: item.title,
                views: item.views
            })
